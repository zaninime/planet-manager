{
  "name": "planet-manager",
  "version": "17.1.0",
  "description": "ELOS Planet Manager application for Android, iOS and Electron",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development WP_TARGET=electron webpack-dev-server --config webpack.config.babel.js --progress --inline",
    "start-android": "cross-env NODE_ENV=development WP_TARGET=android webpack-dev-server --config webpack.config.babel.js --progress --inline",
    "start-ios": "cross-env NODE_ENV=development WP_TARGET=ios webpack-dev-server --config webpack.config.babel.js --progress --inline",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.babel.js --progress",
    "clean": "rimraf dist",
    "test": "jest --config jest.config.json --colors",
    "lint": "eslint --cache --color --format codeframe .",
    "flow": "flow check",
    "ci:build": "rimraf dist && NODE_ENV=production webpack --config webpack.config.babel.js",
    "ci:build-fast": "rimraf dist && NODE_ENV=production WP_FASTBUILD=1 webpack --config webpack.config.babel.js",
    "ci:test": "jest --config jest.config.json --colors",
    "ci:lint": "eslint --color ."
  },
  "license": "GPL-2.0",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-closure-elimination": "^1.1.14",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "chokidar": "^1.6.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype-errors": "^1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "events": "^1.1.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.0",
