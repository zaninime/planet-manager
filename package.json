    "react-hammerjs": "^0.5.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-native-vector-icons": "^3.0.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-svg-loader": "^1.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.21.0",
