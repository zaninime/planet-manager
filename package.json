    "json-loader": "^0.5.4",
    "left-pad": "^1.1.3",
    "lodash": "^4.17.2",
    "material-ui": "^0.16.4",
    "node-uuid": "^1.4.7",
    "radium": "^0.18.1",
    "raven-js": "^3.8.1",
    "react": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
